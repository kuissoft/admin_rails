<div class="row">
  <div class="col-lg-12">
    <h3 class="page-header text-asbestos">Sign in</h3>
  </div>
</div>
<%= form_for(:user, path: new_user_session_path) do |f| %>
  <div>
  	<%= f.label :email %>
    <br />
    <%= f.email_field :email, :autofocus => true %>
  </div>
  <div>
  	<%= f.label :password %>
    <br />
    <%= f.password_field :password %>
  </div>
  <%= f.submit 'Log in' %>
<% end %>