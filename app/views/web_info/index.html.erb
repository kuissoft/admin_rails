<div class="row">
  <div class="col-lg-12">
    <h3 class="page-header text-asbestos">Newsletter</h3>
  </div>
</div>

<table>
  <tr>
    <th>ID</th>
    <th>E-MAIL</th>
    <th>CREATED AT</th>
    <th></th>
  </tr>
  <% @signups.each do |signup| %>
    <tr>
      <td><%= signup['id'] %></td>
      <td><%= signup['email'] %></td>
      <td><%= l(signup['created_at'].to_datetime, format: "%d.%m.%Y %H:%M") %></td>
      <td><%= link_to 'Destroy', web_info_path(id: signup['id'], model: 'signups'), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end -%>
</table>
